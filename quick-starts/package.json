{
  "name": "d3qs",
  "version": "1.0.0",
  "description": "Data visualisation",
  "private": true,
  "main": "index.js",
  "type": "module",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "indent_style": "space",
    "semi": false,
    "singleQuote": true,
    "max_line_length": 150
  },
  "scripts": {
    "format": "prettier --single-quote ",
    "buildProd": "tsc && webpack --config webpack.config.js --mode production",
    "buildDev": "webpack --config webpack.config.js --mode development && tsc",
    "buildTSC": "tsc",
    "build": "npm run addDirectory && npm run buildProd && npm run buildDev && tsc",
    "test": "jest",
    "testDebug": "jest --debug",
    "watch": "npm run addDirectory && tsc --watch",
    "addDirectory": "if not exist ..\\charts\\node_modules\\d3qs (mkdir ..\\charts\\node_modules\\d3qs)"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "test"
  ],
  "author": "mark lester",
  "license": "ISC",
  "dependencies": {
    "d3": "^7.9.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.13",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "jest": "29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
